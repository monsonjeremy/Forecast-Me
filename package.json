{
  "name": "forecast-me",
  "version": "1.0.0",
  "license": "MIT",
  "description": "Forecasting Aggregation Site",
  "repository": "https://github.com/monsonjeremy/Forecast-Me",
  "main": "index.js",
  "author": "Jeremy Monson",
  "dependencies": {
    "aws-sdk": "^2.112.0",
    "axios": "^0.16.2",
    "babel-cli": "^6.26.0",
    "babel-runtime": "6.26.0",
    "body-parser": "~1.18.1",
    "chalk": "2.1.0",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.4",
    "d3": "^4.9.1",
    "debug": "~3.0.1",
    "dotenv": "4.0.0",
    "express": "^4.15.4",
    "flow": "^0.2.3",
    "forever": "^0.15.3",
    "fs-extra": "4.0.2",
    "global": "^4.3.2",
    "history": "^4.6.3",
    "jquery": "^3.2.1",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "morgan": "~1.8.1",
    "nodemon": "^1.12.0",
    "npm-run-all": "^4.1.1",
    "object-assign": "4.1.1",
    "promise": "8.0.1",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-burger-menu": "^2.1.6",
    "react-collapse": "^4.0.3",
    "react-dev-utils": "^4.0.1",
    "react-dom": "^15.6.1",
    "react-error-overlay": "1.0.7",
    "react-motion": "^0.5.1",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.1.1",
    "react-router-redux": "next",
    "react-scripts": "1.0.13",
    "recompose": "^0.25.0",
    "redux": "^3.7.1",
    "redux-actions": "^2.2.1",
    "redux-burger-menu": "^0.2.1",
    "redux-devtools": "^3.4.0",
    "redux-thunk": "^2.2.0",
    "webpack": "^3.5.6",
    "whatwg-fetch": "2.0.3"
  },
  "devDependencies": {
    "autoprefixer": "7.1.4",
    "babel-core": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "21.0.2",
    "babel-loader": "^7.1.2",
    "babel-preset-react-app": "^3.0.2",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "css-loader": "^0.28.7",
    "eslint": "4.7.1",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-config-react-app": "1.0.5",
    "eslint-loader": "1.9.0",
    "eslint-plugin-compat": "^1.0.4",
    "eslint-plugin-flowtype": "^2.35.1",
    "eslint-plugin-import": "^2.6.1",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "^7.1.0",
    "exports-loader": "^0.6.4",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "0.11.2",
    "flow-bin": "^0.54.1",
    "html-webpack-plugin": "^2.30.1",
    "husky": "^0.14.3",
    "imports-loader": "^0.7.1",
    "jest": "21.1.0",
    "node-sass": "^4.5.3",
    "node-sass-chokidar": "0.0.3",
    "optimize-css-assets-webpack-plugin": "^3.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "^2.0.6",
    "resolve-url-loader": "^2.1.0",
    "sass-loader": "^6.0.6",
    "shebang-command": "^1.2.0",
    "style-loader": "^0.18.2",
    "sw-precache-webpack-plugin": "^0.11.4",
    "url-loader": "^0.5.9",
    "webpack-dev-server": "^2.7.1",
    "webpack-manifest-plugin": "1.3.2"
  },
  "scripts": {
    "start-js": "node client/scripts/start.js",
    "start": "npm-run-all -p watch-css start-js",
    "start:server:dev": "./node_modules/.bin/forever ./node_modules/.bin/nodemon -L --debug=5050 --watch ./server/ --exec ./node_modules/.bin/babel-node ./server/server.js",
    "start:server:prod": "./node_modules/.bin/forever ./node_modules/.bin/nodemon -L --watch ./server/ --exec ./node_modules/.bin/babel-node ./server/server.js",
    "build": "npm run build-css && node client/scripts/build.js",
    "test": "eslint client/src && flow",
    "flow": "flow",
    "precommit": "yarn test",
    "prepush": "yarn test",
    "build-css": "node-sass-chokidar --include-path ./client/src --include-path ./node_modules client/src/ -o client/src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./client/src --include-path ./node_modules client/src/ -o client/src/ --watch --recursive",
    "docker:build:local": "docker-compose -f ./docker-compose/local.yml -p forecast-me up --build",
    "docker:build:prod": "npm run build && docker-compose -f ./docker-compose/prod.yml up --build"
  },
  "browserslist": [
    "> 1%"
  ],
  "jest": {
    "collectCoverageFrom": [
      "client/src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>client/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/client/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/client/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost:3005",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/client/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    }
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  }
}
